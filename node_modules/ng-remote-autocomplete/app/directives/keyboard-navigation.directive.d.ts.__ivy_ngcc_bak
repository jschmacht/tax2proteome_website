import { ElementRef } from "@angular/core";
import { ItemListService } from "../services/item-list.service";
import { AutocompleteService } from "../services/autocomplete.service";
export declare class KeyboardNavigationDirective {
    private autocompleteService;
    private itemListService;
    private element;
    private readonly scrollService;
    stopEnterPropagation: boolean;
    constructor(autocompleteService: AutocompleteService, itemListService: ItemListService, element: ElementRef);
    onKeydown(event: KeyboardEvent): void;
    private handleDownKey(event);
    private handleUpKey(event);
    private handleEnter(event);
}
